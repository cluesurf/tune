{
  "name": "tune",
  "displayName": "tune",
  "description": "Cross-Cultural Writing System",
  "license": "Apache-2.0",
  "publisher": "teamtreesurf",
  "repository": {
    "type": "git",
    "url": "https://github.com/teamtreesurf/tune.git"
  },
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "configurationDefaults": {
      "[tune]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": false,
        "editor.fontFamily": "ToneEtch"
      }
    },
    "languages": [
      {
        "id": "tune",
        "aliases": [
          "Tune",
          "tune"
        ],
        "extensions": [
          ".tune"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tune",
        "scopeName": "source.tune",
        "path": "./syntaxes/tune.tmLanguage.json"
      }
    ]
  },
  "version": "2.0.0",
  "devDependencies": {
    "csv-parse": "^5.3.0",
    "marked": "^4.1.1",
    "puppeteer": "^18.0.5"
  }
}
